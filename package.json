{
  "name": "react-base",
  "version": "0.0.1",
  "description": "React boilerplate for modern web projects.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "npm run lint && NODE_ENV=production webpack --config webpack.config.js --mode production",
    "build:nocheck": "NODE_ENV=production webpack --config webpack.config.js --mode production",
    "serve": "http-server ./build -g",
    "clean": "rm -rf node_modules",
    "lint": "eslint \"**/*.js[x]\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tlima/react-base.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "jsx",
    "webpack",
    "redux",
    "jest"
  ],
  "author": "Thiago L. Lima",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.4.5",
    "@babel/preset-react": "~7.0.0",
    "babel-loader": "~8.0.6",
    "clean-webpack-plugin": "~3.0.0",
    "compression-webpack-plugin": "^3.0.1",
    "css-loader": "~2.0.0",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-import-resolver-webpack": "~0.11.1",
    "eslint-plugin-import": "~2.17.3",
    "eslint-plugin-react": "~7.13.0",
    "fibers": "~4.0.1",
    "file-loader": "~4.0.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.7.0",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "sass": "~1.21.0",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "terser-webpack-plugin": "~1.3.0",
    "webpack": "~4.34.0",
    "webpack-cli": "~3.3.4",
    "webpack-dev-server": "~3.7.1"
  },
  "dependencies": {
    "prop-types": "~15.7.2",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-redux": "~7.1.0",
    "react-router-dom": "~5.1.2",
    "redux": "~4.0.1"
  }
}
