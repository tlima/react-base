{
  "name": "react-base",
  "version": "1.0.0",
  "description": "React boilerplate for modern web projects.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --progress",
    "build": "npm run lint && NODE_ENV=production webpack --config webpack.config.js --mode production",
    "build:nocheck": "NODE_ENV=production webpack --config webpack.config.js --mode production",
    "serve": "http-server ./dist -g",
    "clean": "rm -rf node_modules && rm -rf dist",
    "eslint": "eslint --ext .jsx,.js ./",
    "stylelint": "stylelint './src/**/*.js'",
    "lint": "npm run eslint && npm run stylelint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tlima/react-base.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "styled components",
    "redux",
    "saga",
    "webpack"
  ],
  "author": "Thiago L. Lima",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.15.5",
    "@babel/preset-react": "~7.14.5",
    "babel-loader": "~8.2.2",
    "babel-plugin-styled-components": "~1.13.2",
    "clean-webpack-plugin": "~4.0.0",
    "compression-webpack-plugin": "~8.0.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "~7.32.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-import-resolver-webpack": "~0.13.1",
    "eslint-plugin-import": "~2.24.2",
    "eslint-plugin-react": "~7.25.1",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "~6.2.0",
    "html-webpack-plugin": "~5.3.2",
    "path": "~0.12.7",
    "style-loader": "~3.2.1",
    "stylelint": "~13.13.1",
    "stylelint-config-recommended": "~5.0.0",
    "stylelint-config-styled-components": "~0.1.1",
    "stylelint-processor-styled-components": "~1.10.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "terser-webpack-plugin": "~5.2.3",
    "webpack": "~5.52.0",
    "webpack-cli": "~4.8.0",
    "webpack-dev-server": "~4.1.1"
  },
  "dependencies": {
    "prop-types": "~15.7.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-redux": "~7.2.1",
    "react-router-dom": "~5.3.0",
    "redux": "~4.1.1",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "styled-components": "~5.3.1"
  }
}
